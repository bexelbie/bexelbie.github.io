source 'https://rubygems.org'

group :jekyll_plugins do
  # These are from the _config.yml
  gem "jekyll-paginate"
  # This needed for proper execution and not built in
  # Issue opened: https://github.com/jekyll/docker/issues/79
  gem "jekyll-watch"
  # These built-in to the docker container
  gem "jekyll-feed"
  gem "kramdown"
end
