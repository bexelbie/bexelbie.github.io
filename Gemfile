group :jekyll_plugins do
  gem "jekyll-paginate"
  gem "jekyll-feed"
  # This needed for proper execution of the docker container and built in
  gem "kramdown"
  # This needed for proper execution of the docker container and not built in
  # Issue opened: https://github.com/jekyll/docker/issues/79
  gem "jekyll-watch"
end
